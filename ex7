#include <stdio.h>

int main(void) {
  float x, y;
  printf (" Digite um número: ");
  scanf("%f", &x);
  if (x <= 1) { // define um valor para y em funcao do x colocado pelo usuario
    y=1;
  } else if (1<x && x<=2){
    y=2;
  } else if (2<x && x<=3){
    y=x*x;
  }else {
    y=x*x*x;
  }
  printf ("o Y é: %.1f", y);
  return 0;
}
